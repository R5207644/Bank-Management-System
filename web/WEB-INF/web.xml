<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>BankServlet.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>BankServlet.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>BankServlet.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>BankServlet.Index</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>BankServlet.Delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/Delete</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Notice</servlet-name>
        <servlet-class>BankServlet.Notice</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Notice</servlet-name>
        <url-pattern>/Notice</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Withdraw</servlet-name>
        <servlet-class>BankServlet.Withdraw</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Withdraw</servlet-name>
        <url-pattern>/Withdraw</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>deposite</servlet-name>
        <servlet-class>BankServlet.Deposite</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deposite</servlet-name>
        <url-pattern>/Deposite</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>Transfer</servlet-name>
        <servlet-class>BankServlet.Transfer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Transfer</servlet-name>
        <url-pattern>/Transfer</url-pattern>
    </servlet-mapping>
</web-app>
